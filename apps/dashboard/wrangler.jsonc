{
  "name": "kuratchi",
  "account_id": "290c1a3b978d0f44f83b515983a1c951",
  "main": ".svelte-kit/cloudflare/_worker.js",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "compatibility_date": "2025-09-24",
  "assets": {
    "binding": "ASSETS",
    "directory": ".svelte-kit/cloudflare"
  },
  "build": {
    "command": "pnpm run build"
  },
  "placement": {
    "mode": "smart"
  },
  "kv_namespaces": [
    {
      "binding": "KV",
      "id": "8300ba36ac64476d8c44579056d57e78"
    }
  ],
  "d1_databases": [
    {
      "binding": "ADMIN_DB",
      "database_name": "kayde-admin-db",
      "database_id": "463a4d89-599f-4c7b-93b9-62514c6eeb65"
    }
  ],
  "r2_buckets": [
    {
      "binding": "BUCKET",
      "bucket_name": "kayde-admin-bucket",
      "experimental_remote": true
    }
  ],
  "observability": {
    "enabled": true
  },
  "ai": {
    "binding": "AI"
  },
  "browser": {
    "binding": "BROWSER"
  },
  "vectorize": [
    {
      "binding": "VECTORIZE",
      "index_name": "embeddings-index"
    }
  ],
  "keep_vars": true,
  "vars": {
    "ORIGIN": "https://builder.kayde.io",
    "APP_LOGIN_PATH": "/app/connect",
    "APP_SIGNUP_PATH": "/app/connect"
  }
}