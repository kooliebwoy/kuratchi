# Cloudflare API credentials
CF_API_TOKEN=
CF_ACCOUNT_ID=

# Workers subdomain (e.g. acct.workers.dev)
WORKERS_SUBDOMAIN=

# Per-database worker bearer token (created during provisioning)
WORKER_BEARER_TOKEN=

# Database name for testing (worker script expects <db>.<workersSubdomain>)
DB_NAME=

# Optional: provisioning controls
# Set to 1 to create DB + deploy default worker before querying
PROVISION=
# PrimaryLocationHint: wnam | enam | weur | eeur | apac | oc
LOCATION=weur
